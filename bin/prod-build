#!/usr/bin/env bash

set -Eeuo pipefail

# Netlify uses an older version of clojure CLI, hence the outdated invocation styles.

rm -rf target && mkdir target
echo Importing mbrainz schema...
clojure -A:netlify -m ingest.projects.mbrainz
echo Building standalone HTML
clojure -A:netlify -m build.standalone
mv target/standalone.html target/index.html
